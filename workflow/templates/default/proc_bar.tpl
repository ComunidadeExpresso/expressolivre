<table cellpadding='0' cellspacing='0' width="100%" style="border:1px solid black">
<tr>
	<td>
		<div style="height:20px; padding-top:2px; padding-bottom:2px;background-color:white; color:black; font-size:10px;">
			<table  cellpadding="0" cellspacing="0" width="100%" border="0">
				<tr>
					<td>
						<b>{proc_name}:{version}</b>
						{lang_validity:}&nbsp;<img border="0" src="{img_validity}" alt="{alt_validity}" title="{alt_validity}">
					</td>
					<td align='right'>
						<table cellpadding='0' cellspacing='2' width="100%">
							<tr>
								{start_stop}
								<td><a href="{link_admin_activities}"><img border="0" src="{img_activity}" alt="{lang_Activities}" title="{lang_Activities}" />{lang_Activities}</a></td>
								<td><a href="{link_admin_processes}"><img border="0" src="{img_change}" alt="{lang_Edit}" title="{lang_Edit}" />{lang_Edit}</a></td>
								<td><a href="{link_admin_shared_source}"><img border="0" src="{img_code}" alt="{lang_Code}" title="{lang_Code}" />{lang_Code}</a></td>
								<td><a href="{link_admin_roles}"><img border="0" src="{img_roles}" alt="{lang_Roles}" title="{lang_Roles}" />{lang_Roles}</a></td>
								<td><a href="{link_admin_jobs}"><img border="0" src="{img_job}" alt="{lang_Jobs}" title="{lang_Jobs}" />{lang_Jobs}</a></td>
								<td><a href="{link_graph}" target="_external"><img src="{img_process}" border="0" title="{lang_Graph}" alt="{lang_Graph}" />{lang_Graph}</a></td>
								<td><a href="{link_admin_export}"><img border="0" src="{img_save}" alt="{lang_Export}" title="{lang_Export}" />{lang_Export}</a></td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
		</div>
	</td>
</tr>
</table>
