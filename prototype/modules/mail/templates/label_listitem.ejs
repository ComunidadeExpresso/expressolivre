	<li class="label-item label-item-<%= data.id%>" style="background: <%= (data.bgColor) ? data.bgColor : '#ebebeb' %>; 		color: <%= (data.fontColor) ? data.fontColor : '#000000' %>;		border: 1px solid <%= (data.borderColor) ? data.borderColor : '#000000' %>;">					<input type="hidden" value="<%= data.id%>" name="labelItemId" class="id-item-list" />		<span class="text-list"><%= data.name %></span>		<div class="button close tiny" style="float: right" title="<%= get_lang("Delete")%>"></div>		<div class="button edit tiny" style="float: right" title="<%= get_lang("Edit")%>"></div>						</li>	