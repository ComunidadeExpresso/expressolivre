<% if(data){ 
	var priority = {1: 'Alta', 2: 'Normal', 3: 'Baixa'};%>
	Título: <%= data.summary %><br>
	Início: <%= data.startDate %> <%= data.startHour %><br>
	Prev. Fim: <%= data.dueDate %> <%= data.dueTime %><br>
	% Conclusão: <%= data.percentage %><br>
	Prioridade: <%= priority[data.priority] %><br>
<% } %>
