<% for (var i=0; i < data.length; i++) { %><li class="ui-widget-content" title="<%=data[i]['name']%>" style="border: 0;">	<input type="checkbox" name="option" <%= (data[i].enabled) ?  '' : 'disabled="disabled"' %> value="<%= data[i].id %>" />    <input type="hidden" name="id" value="<%=data[i]['id']%>" />    <input type="hidden" name="taskStartDate" value="<%=data[i]['startDate']%>" />    <input type="hidden" name="taskStartHour" value="<%=data[i]['startHour']%>" />    <input type="hidden" name="taskEndDate" value="<%=data[i]['endDate']%>" />    <input type="hidden" name="taskEndHour" value="<%=data[i]['endHour']%>" />    <input type="hidden" name="taskDueDate" value="<%=data[i]['dueDate']%>" />    <input type="hidden" name="taskDueTime" value="<%=data[i]['dueTime']%>" />    <input type="hidden" name="taskAllDay" value="<%=data[i]['allDay']%>" />    <input type="hidden" name="taskStatus" value="<%=data[i]['status']%>" />    <input type="hidden" name="taskPercentage" value="<%=data[i]['percentage']%>" />    <input type="hidden" name="taskPriority" value="<%=data[i]['priority']%>" />    <input type="hidden" name="taskSummary" value="<%=data[i]['summary']%>" />    <label class="description"><%=data[i]['startDate']%> - <%=data[i]['description']%></label></li><% } %>